<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.volkankelleci.petsocialclub.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_mainFragment_to_usersHomeFragment"
            app:destination="@id/usersHomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.volkankelleci.petsocialclub.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFillFragment"
        android:name="com.volkankelleci.petsocialclub.profileviewfills.ProfileFillFragment"
        android:label="fragment_profile_fill"
        tools:layout="@layout/fragment_profile_fill" >
        <action
            android:id="@+id/action_profileFillFragment_to_usersHomeFragment"
            app:destination="@id/usersHomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.volkankelleci.petsocialclub.navbotviews.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" />
    <fragment
        android:id="@+id/messageFragment"
        android:name="com.volkankelleci.petsocialclub.MessageFragment"
        android:label="fragment_message"
        tools:layout="@layout/fragment_message" >
        <action
            android:id="@+id/action_messageFragment_to_usersHomeFragment"
            app:destination="@id/usersHomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/usersHomeFragment"
        android:name="com.volkankelleci.petsocialclub.UsersHomeFragment"
        android:label="fragment_users_home"
        tools:layout="@layout/fragment_users_home" >
        <action
            android:id="@+id/action_usersHomeFragment_to_profileFillFragment"
            app:destination="@id/profileFillFragment" />
        <action
            android:id="@+id/action_usersHomeFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
        <action
            android:id="@+id/action_usersHomeFragment_self"
            app:destination="@id/usersHomeFragment" />
        <action
            android:id="@+id/action_usersHomeFragment_to_messageFragment"
            app:destination="@id/messageFragment" />
        <action
            android:id="@+id/action_usersHomeFragment_self2"
            app:destination="@id/usersHomeFragment" />
        <action
            android:id="@+id/action_usersHomeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.volkankelleci.petsocialclub.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>